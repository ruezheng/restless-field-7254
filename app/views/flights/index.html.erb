<h1>Flights Index Page</h1>

<% @flights.each do |flight| %>
  <hr><div id="flight-<%= flight.id %>">
    <h3>Flight <%= flight.number %></h3>
    <p><%= flight.airline.name %> Passengers:</p>
    <% flight.passengers.each do |passenger| %>
    <ol><%= passenger.name %></ol>
    <% end %>
  </div>
<% end %>
